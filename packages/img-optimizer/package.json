{
  "name": "img-optimizer",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "image",
    "optimizer"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/nitedani/img-optimizer"
  },
  "scripts": {
    "build": "rimraf lib && tsc -b"
  },
  "exports": {
    ".": "./lib/index.js",
    "./server": "./lib/server/index.js",
    "./client": "./lib/client/index.js"
  },
  "typesVersions": {
