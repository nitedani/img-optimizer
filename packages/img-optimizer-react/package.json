{
  "name": "img-optimizer-react",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "image",
    "optimizer"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/nitedani/img-optimizer"
  },
  "scripts": {
    "build": "rimraf lib && tsc -b"
  },
  "exports": {
    ".": "./lib/index.js",
    "./server": "./lib/server/index.js",
    "./client": "./lib/client/index.js"
  },
  "typesVersions": {
    "*": {
      "server": [
        "lib/server/index.d.ts"
      ],
      "client": [
        "lib/client/index.d.ts"
      ],
      "*": [
        "lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "img-optimizer": "*"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  }
}
